{
   "name": "TCC",
   "version": "1.0.0",
   "main": "index.js",
   "license": "MIT",
   "scripts": {
      "lint": "eslint . --ext .ts",
      "start": "ts-node -r tsconfig-paths/register src/main/server.ts",
      "build": "tsc",
      "test": "if [ \"$RUN_TESTS\" != \"false\" ]; then jest --passWithNoTests --runInBand --no-cache",
      "test:unit": "yarn test -- --watch -c jest-unit-config.js",
      "typeorm:migration:create": "yarn typeorm migration:create ./src/infra/db/postgres/migrations/$MIGRATION_NAME",
      "typeorm:migration:run": "yarn typeorm migration:run -d ./ormconfig.json"
   },
   "dependencies": {
      "bcrypt": "^5.1.0",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "jsonwebtoken": "^9.0.0",
      "mysql2": "^3.3.2",
      "path": "^0.12.7",
      "pg": "^8.11.0",
      "reflect-metadata": "^0.1.13",
      "sequelize": "^6.31.1",
      "sequelize-typescript": "^2.1.5",
      "uuid": "^9.0.0",
      "validator": "^13.9.0"
   },
   "devDependencies": {
      "@shelf/jest-mongodb": "^4.1.7",
      "@types/bcrypt": "^5.0.0",
      "@types/chance": "^1.1.3",
      "@types/express": "^4.17.17",
      "@types/jest": "^29.5.0",
      "@types/jsonwebtoken": "^9.0.1",
      "@types/node": "^20.2.3",
      "@types/uuid": "^9.0.1",
      "@types/validator": "^13.7.17",
      "@typescript-eslint/eslint-plugin": "^5.59.0",
      "@typescript-eslint/parser": "^5.59.0",
      "chance": "^1.1.11",
      "eslint": "^8.38.0",
      "jest": "^29.5.0",
      "sequelize-cli": "^6.6.0",
      "ts-jest": "^29.1.0",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "tsconfig-paths": "^4.2.0",
      "typescript": "4.5.2"
   }
}
